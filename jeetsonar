node{
   stage('SCM Checkout'){
     git 'https://github.com/jasansh/my-app'
   }
   stage('Compile-Package'){
      // Get maven home path
      // def mvnHome =  tool name: 'maven-3', type: 'maven'   
      sh "mvn package"
   }
   
   stage('SonarQube Analysis') {
        // def mvnHome =  tool name: 'maven-3', type: 'maven'
        withSonarQubeEnv('sonar-9') { 
          sh "mvn sonar:sonar"
        }
    }
}
